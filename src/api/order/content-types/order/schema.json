{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "TableNumber": {
      "type": "integer"
    },
    "status_food": {
      "type": "enumeration",
      "enum": [
        "pending",
        "cooking",
        "ready",
        "picked",
        "served"
      ]
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu.menu",
      "mappedBy": "order"
    },
    "waiter": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::menu.menu",
      "inversedBy": "orders"
    }
  }
}
